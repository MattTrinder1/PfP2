{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "fileLoggingMode": "always",
      "logLevel": {
        "default": "Information"
      }
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:01:00",
      "visibilityTimeout": "00:00:15",
      "batchSize": 2,
      "maxDequeueCount": 4,
      "newBatchThreshold": 8
    }
  }
}